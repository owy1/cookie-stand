<p>lab6_cookie stand</p>

<p id="demo"></p>

<script>
var Alki = {
  minCustperDay = 2;
  maxCustperDay = 16;
  avgCookieEaCust = 4.6;
};

Alki.prototype.generateRandom = function(min,max) {
  return Math.floor(Math.random() * (max-min+1)) + min;
}

Alki.prototype.soldHourly = [];
Alki.prototype.cookieSoldEaHr = function() {
  var cookiePerHr, custPerHr;


  for (var i=0,i<15,i++) {
  cookiePerHr = Math.round(this.generateRandom(minCustperDay,maxCustperDay) * this.avgCookieEaCust);
  Alki.prototype.soldHourly.push(cookiePerHr);
}
var cookieTot = 0;
    for (var i=0;i<15;i++) {
      cookieTot += this.cookiePerHr();
    }
    return cookieTot;
}

console.log(Alki.prototype.cookieTot);

</script>

